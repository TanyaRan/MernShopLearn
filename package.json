{
  "name": "merntravel",
  "version": "1.0.0",
  "description": "MERN project - Travel",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server/server",
    "server": "nodemon server/server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node server/seeder",
    "data:destroy": "node server/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TanyaRan/MernTravel.git"
  },
  "author": "TanyaTsv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TanyaRan/MernTravel/issues"
  },
  "homepage": "https://github.com/TanyaRan/MernTravel#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  }
}
